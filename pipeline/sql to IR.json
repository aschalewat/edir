{
	"name": "sql to IR",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "PostgreSqlV2Source"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "title",
									"type": "String",
									"physicalType": "character varying"
								},
								"sink": {
									"name": "title",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "price",
									"type": "Double",
									"physicalType": "double precision"
								},
								"sink": {
									"name": "price",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "author_id",
									"type": "Int64",
									"physicalType": "bigint"
								},
								"sink": {
									"name": "author_id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "cat_id",
									"type": "Int64",
									"physicalType": "bigint"
								},
								"sink": {
									"name": "cat_id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "description",
									"type": "String",
									"physicalType": "character varying"
								},
								"sink": {
									"name": "description",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "book_id",
									"type": "Int32",
									"physicalType": "integer"
								},
								"sink": {
									"name": "book_id",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "PostgreSqlTable_ds",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "bookcsv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}